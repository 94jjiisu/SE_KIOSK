        _translate = QtCore.QCoreApplication.translate
        menuManage.setWindowTitle(_translate("menuManage", "Dialog"))
        self.name_label.setText(_translate("menuManage", "메뉴 추가 및 변경"))
        self.label_11.setText(_translate("menuManage", "메뉴 이름"))
        self.label_12.setText(_translate("menuManage", "메뉴 수량"))
        self.label_2.setText(_translate("menuManage", "메뉴 금액"))
        self.returnBtn.setText(_translate("menuManage", "취소"))
        self.saveBtn.setText(_translate("menuManage", "저장"))

#Window class
class initialPage(QtWidgets.QWidget): #초기화면
    def __init__(self, parent=None):
        super().__init__(parent=parent)
        orderUi = initialUi()
        orderUi.setupUi(self)

        orderUi.orderBtn.clicked.connect(self.openMain)
        orderUi.managerBtn.clicked.connect(self.openManager)

        self.setFixedSize(500, 900)

    def openMain(self):
        threading.Timer(120, self.returnInitial).start() # 메인화면으로 넘어 가는 버튼 누른 후 2분간 주문 끝내지 않으면 자동으로 초기 화면으로 돌아감
        self.parent().stack.setCurrentIndex(1) #메인화면으로 전환

    def openManager(self):
        self.parent().stack.setCurrentIndex(8) #관리자모드로 전환
        
    def returnInitial(self):
        MainWindow.totalCost = 0
        MainWindow.menuName = ""
        MainWindow.topping = "없음"
        MainWindow.bread = "기본"

        self.parent().stack.setCurrentIndex(0) #초기 화면으로 전환

class order(QtWidgets.QWidget): #주문하는 메인화면

    def __init__(self, parent=None):
        super().__init__(parent=parent)
        menumainUi = mainUi()
        menumainUi.setupUi(self)

        menumainUi.misterEggBtn.clicked.connect(self.mrOpen)
        menumainUi.americanHamBtn.clicked.connect(self.americanOpen)
        menumainUi.baconDoubleBtn.clicked.connect(self.baconOpen)

        self.setFixedSize(500,900)

    def mrOpen(self):
        orderDetail.orderUi.nameLabel.setText("미스터에그")
        orderDetail.orderUi.detailLabel.setText("기본에 충실한\n"
                                                "그 맛.\n")
        self.parent().stack.setCurrentIndex(2) #상세화면으로 전환

    def americanOpen(self):
        orderDetail.orderUi.nameLabel.setText("아메리칸 햄치즈")
        orderDetail.orderUi.detailLabel.setText("아메리카에서 온\n"
                                                "햄과\n"
                                                "치즈\n")
        self.parent().stack.setCurrentIndex(2) #상세화면으로 전환

    def baconOpen(self):
        orderDetail.orderUi.nameLabel.setText("베이컨 더블치즈")
        orderDetail.orderUi.detailLabel.setText("베이컨과\n"
                                                "치즈의\n"
                                                "만남♥\n")
        self.parent().stack.setCurrentIndex(2) #상세화면으로 전환

class orderDetail(QtWidgets.QWidget): #상세정보 화면

    orderUi = menuUi()

    def __init__(self, parent=None):
        super().__init__(parent=parent)
        #ui = menuUi()
        ui = orderDetail.orderUi
        ui.setupUi(self)

        ui.returnBtn.clicked.connect(self.returnMain) #메뉴 선택 화면으로
        ui.okBtn.clicked.connect(self.doPay) #포장/매장 선택화면으로

        self.setFixedSize(500,900)

    def returnMain(self):
        ui = orderDetail.orderUi

        ui.avocadoBtn.setCheckState(False)
        ui.pollackBtn.setCheckState(False)
        ui.eggBtn.setCheckState(False)
        ui.bunBtn.setCheckState(False)

        MainWindow.totalCost = 0
        MainWindow.menuName = ""
        MainWindow.topping = "없음"
        MainWindow.bread = "기본"

        self.parent().stack.setCurrentIndex(1)

    def doPay(self):
        ui = orderDetail.orderUi

        if ui.nameLabel.text() == "미스터에그":
            MainWindow.menuName = "미스터에그"
            MainWindow.totalCost += 3400
        elif ui.nameLabel.text() == "아메리칸 햄치즈":
            MainWindow.menuName = "아메리칸 햄치즈"
            MainWindow.totalCost += 3900
        elif ui.nameLabel.text() == "베이컨 더블치즈":
            MainWindow.menuName = "베이컨 더블치즈"
            MainWindow.totalCost += 4200

        if ui.bunBtn.isChecked():
            MainWindow.bread = "번"

        if ui.eggBtn.isChecked():
            MainWindow.topping = "계란"
            MainWindow.totalCost += 200

        if ui.pollackBtn.isChecked():
            MainWindow.topping = "명란"
            MainWindow.totalCost += 500

        if ui.avocadoBtn.isChecked():
            MainWindow.topping = "아보카도"
            MainWindow.totalCost += 700

        print("메뉴: ", MainWindow.menuName)
        print("빵: ", MainWindow.bread)
        print("토핑: ", MainWindow.topping)
        print("가격: ", MainWindow.totalCost)

        self.parent().stack.setCurrentIndex(3) #포장/매장 선택화면으로 전환

class eatHere(QtWidgets.QWidget):

    takeOut = takeoutUi()

    def __init__(self, parent=None):
        super().__init__(parent=parent)
        ui= eatHere.takeOut
        ui.setupUi(self)

        ui.returnBtn.clicked.connect(self.returnMenu)
        ui.inBtn.clicked.connect(self.nextPage)
        ui.outBtn.clicked.connect(self.changeState)

        self.setFixedSize(500,900)

    def returnMenu(self):
        MainWindow.totalCost = 0
        MainWindow.menuName = ""
        MainWindow.topping = "없음"
        MainWindow.bread = "기본"
        self.parent().stack.setCurrentIndex(2)

    def changeState(self):
        MainWindow.eatHere = False
        self.nextPage()

    def nextPage(self):
        checkOrder.ordercheckUi.totalLabel.setText(str(MainWindow.totalCost))

        menuInformation = "    " + MainWindow.menuName + "\t" + MainWindow.topping + "\t" + MainWindow.bread + "\t" + str(MainWindow.totalCost)

        menulist = QtGui.QStandardItemModel(self)

        menuItem = QtGui.QStandardItem(menuInformation)

        menulist.appendRow(menuItem)

        checkOrder.ordercheckUi.menuList.setModel(menulist)

        self.parent().stack.setCurrentIndex(4) #주문내역 확인으로 전환

class checkOrder(QtWidgets.QWidget):

    ordercheckUi = checkUi()

    def __init__(self, parent=None):
        super().__init__(parent=parent)
        ui = checkOrder.ordercheckUi
        ui.setupUi(self)

        #self.changeList()

        ui.returnBtn.clicked.connect(self.returnHere)
        ui.okBtn.clicked.connect(self.pay)

        self.setFixedSize(500,900)

    def returnHere(self):
        MainWindow.eatHere = True #디폴트값인 매장 식사로 변경
        self.parent().stack.setCurrentIndex(3) #포장/매장 선택화면으로 돌아감

    def pay(self):
        self.parent().stack.setCurrentIndex(5)

    def changeList(self):
        checkOrder.ordercheckUi.totalLabel.setText(str(MainWindow.totalCost))

class payment(QtWidgets.QWidget): #결제화면

    payUi = paymentUi()
    payOk = False

    def __init__(self, parent=None):
        super().__init__(parent=parent)
        ui = payment.payUi
        ui.setupUi(self)

        ui.returnBtn.clicked.connect(self.returnHere)
        ui.moenyInput.returnPressed.connect(self.checkMoney)
        payment.payUi.moenyInput.setText(ui.moenyInput.text())

        self.setFixedSize(500,900)

    def returnHere(self):
        payment.payUi.moenyInput.setText("")
        self.parent().stack.setCurrentIndex(3) #매장/포장 선택 화면으로 돌아감

    def checkMoney(self):

        money = payment.payUi.moenyInput.text()
        print("입력 가격: ", money)
        print("총 가격: ", str(MainWindow.totalCost))

        if money == str(MainWindow.totalCost):
            success.okUi.ordernumLabel.setText(str(MainWindow.numOrder))
            MainWindow.numOrder += 1
            self.parent().stack.setCurrentIndex(6) #성공 화면
            threading.Timer(5,self.initial).start() #5초 후 초기 화면으로 돌아감
        else:
            self.parent().stack.setCurrentIndex(7) #실패 화면

    def initial(self):
        ui = orderDetail.orderUi

        ui.avocadoBtn.setCheckState(False)
        ui.pollackBtn.setCheckState(False)
        ui.eggBtn.setCheckState(False)
        ui.bunBtn.setCheckState(False)

        MainWindow.totalCost = 0
        MainWindow.menuName = ""
        MainWindow.topping = "없음"
        MainWindow.bread = ""

        payment.payUi.moenyInput.setText("")

        if MainWindow.numOrder >= 1000:
            MainWindow.numOrder = 100

        MainWindow.totalCost = 0
        MainWindow.eatHere = True
        payment.payUi.moenyInput.text()

        self.parent().stack.setCurrentIndex(0) #초기 화면으로 전환

class success(QtWidgets.QWidget):

    okUi = successUi()

    def __init__(self, parent=None):
        super().__init__(parent=parent)
        ui = success.okUi
        ui.setupUi(self)

        self.setFixedSize(500,900)

class fail(QtWidgets.QWidget):
    def __init__(self, parent=None):
        super().__init__(parent=parent)
        ui = failUi()
        ui.setupUi(self)

        ui.returnBtn.clicked.connect(self.returnPay)

        self.setFixedSize(500,900)

    def returnPay(self):
        payment.payUi.moenyInput.setText("")
        self.parent().stack.setCurrentIndex(5) #결제 창으로 돌아감

class manager(QtWidgets.QWidget): #관리자모드
        def __init__(self, parent=None):
        super().__init__(parent=parent)
        ui = managementUi()
        ui.setupUi(self)

        ui.returnBtn.clicked.connect(self.returnInitial)
        ui.plus_btn.clicked.connect(self.addMenu)

        self.setFixedSize(500, 900)

    def returnInitial(self):
        self.parent().stack.setCurrentIndex(0)

    def addMenu(self):
        self.parent().stack.setCurrentIndex(9)

class menuManage(QtWidgets.QWidget): #메뉴 수정
    def __init__(self, parent=None):
        super().__init__(parent=parent)
        ui = menumanageUi()
        ui.setupUi(self)

        ui.returnBtn.clicked.connect(self.returnManagement)
        ui.saveBtn.clicked.connect(self.save)

        self.setFixedSize(500, 900)

    def returnManagement(self):
        self.parent().stack.setCurrentIndex(8)

    def save(self):
        #DB 저장, 수정하는 동작 구현하기!
        self.parent().stack.setCurrentIndex(8)

class MainWindow(QtWidgets.QWidget):

    menuName = ""
    totalCost = 0
    eatHere = True  # T = 매장식사, F = 포장
    numOrder = 100
    bread = "기본"
    topping = "없음"

    if numOrder >= 1000:
        numOrder = 100

    def __init__(self):
        super().__init__()
        self.setFixedSize(500, 900)
        self.setWindowTitle('EGGDROP')
        self.stack = QtWidgets.QStackedLayout(self)
        self.stack0 = initialPage(self)
        self.stack1 = order(self)
        self.stack2 = orderDetail(self)
        self.stack3 = eatHere(self)
        self.stack4 = checkOrder(self)
        self.stack5 = payment(self)
        self.stack6 = success(self)
        self.stack7 = fail(self)
        self.stack8 = manager(self)
        self.stack9 = menuManage(self)
        self.stack.addWidget(self.stack0) #초기화면
        self.stack.addWidget(self.stack1) #메뉴선택화면
        self.stack.addWidget(self.stack2) #메뉴 디테일
        self.stack.addWidget(self.stack3) #포장/매장 선택
        self.stack.addWidget(self.stack4) #주문내역 확인
        self.stack.addWidget(self.stack5) #결제
        self.stack.addWidget(self.stack6) #결제 성공
        self.stack.addWidget(self.stack7) #결제 실패
        self.stack.addWidget(self.stack8) #관리자모드
        self.stack.addWidget(self.stack9) #메뉴 수정
        self.show()

app = QtWidgets.QApplication([])
main = MainWindow()
app.exec()
